<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.loginexam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mobi_svg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_mobi_svg_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginExam"
        android:usesCleartextTraffic="true">
        <activity android:name=".User.JoinStep2"></activity>
        <activity android:name=".User.JoinStep1" />
        <activity android:name=".User.Join" />
        <activity android:name=".MainActivity" />
        <activity android:name=".User.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>